{
	"name": "nimbly/resolve",
	"description": "A simple PSR-11 compliant autowiring and dependency injector.",
	"type": "library",
	"keywords": ["autowiring","dependency injection"],
	"require": {
		"php": "^8.0",
		"psr/container": "^2.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.0",
		"vimeo/psalm": "^4.0",
		"nimbly/carton": "^1.0",
		"symfony/var-dumper": "^5.1",
		"php-coveralls/php-coveralls": "^2.2"
	},
	"license": "MIT",
	"authors": [
		{
			"name": "Brent Scheffler",
			"email": "brent@brentscheffler.com"
		}
	],
	"autoload": {
		"psr-4": {
			"Nimbly\\Resolve\\": "src\/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Nimbly\\Resolve\\Tests\\": "tests\/"
		}
	}
}
